<div class="ctx data-monitor-contaienr">

    <div class="center-header-container" style="background-color: #f8f8f8;">
        <div class="center-header-title">
            <div class="center-header-title-show">观测数据监控</div>
        </div>
        <div class="center-header-query">
            <div class="header-query-search">
                <label class="header-query-search-label">源名 :</label>
				<div id="sname" class="sequence-name header-query-search-input" style="width:280px;" />
            </div>
            <div class="header-query-search">
                <label class="header-query-search-label">任务状态 :</label>
                <div id="taskstate" class="task-state header-query-search-input" />
            </div>
            <div class="header-query-search">
                <label class="header-query-search-label">开始时间 :</label>
                <div id="starttmvalue" class="begin-time header-query-search-input" />
            </div>
            <div class="header-query-search">
                <label class="header-query-search-label">已用时间 :</label>
                <div id="lasttmvalue" class="pass-time header-query-search-input " />
